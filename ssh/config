IdentitiesOnly yes
UseKeychain yes

Host bitbucket.org
  User git
  IdentityFile ~/.ssh/bitbucket.key

Host github.com
  User git
  IdentityFile ~/.ssh/github.key

Host heroku.com
  User git
  IdentityFile ~/.ssh/heroku.key

Host unindented
  HostName unindented.org
  User unindented
  IdentityFile ~/.ssh/unindented.key

Host discoverelectron
  HostName discoverelectron.com
  User discoverelectron
  IdentityFile ~/.ssh/discoverelectron.key
